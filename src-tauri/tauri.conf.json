{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "defcomm-browser",
  "version": "0.1.0",
  "identifier": "com.defcomm.browser",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Defcomm Browser",
        "width": 800,
        "height": 600,
        "minWidth": 600,
        "minHeight": 400,
        "decorations": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32/1.png",
      "icons/32x32/2.png",
      "icons/128x128 ICNS/1 @1x.icns",
      "icons/ICO 128x128/2 @1x.ico",
      "icons/128x128/1x/2 @1x.png",
      "icons/128x128/1x/1 @1x.png",
      "icons/128x128/2x/1@2x.png",
      "icons/icon.png"
    ]
  }
}
